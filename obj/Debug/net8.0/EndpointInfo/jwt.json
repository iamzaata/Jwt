              "schema": {
                "$ref": "#/components/schemas/LoginUser"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUser"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
